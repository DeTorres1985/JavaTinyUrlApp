/*
  this is scratch with creating db sql queries just for future migration service.
  For now db created by Spring.
*/
CREATE DATABASE `urlshortener` /*!40100 DEFAULT CHARACTER SET utf8mb3 */ /*!80016 DEFAULT ENCRYPTION='N' */;

CREATE TABLE `urlshortener`.`tinyurls` (
  `link` VARCHAR(5) NOT NULL,
  `url` VARCHAR(2048) NULL,
  PRIMARY KEY (`link`));


CREATE TABLE `urlshortener`.`free_links` (
  `link` VARCHAR(5) NOT NULL,
  PRIMARY KEY (`link`));


CREATE TABLE `urlshortener`.`paid_links` (
  `link` VARCHAR(5) NOT NULL,
  `price` DECIMAL(10,2) NOT NULL,
  PRIMARY KEY (`link`));
